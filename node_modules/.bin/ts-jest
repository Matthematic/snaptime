#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/matthewcarr_1/Documents/Github/snaptime/node_modules/.pnpm/ts-jest@29.1.1_@babel+core@7.22.10_jest@29.6.3_typescript@5.1.6/node_modules/ts-jest/node_modules:/Users/matthewcarr_1/Documents/Github/snaptime/node_modules/.pnpm/ts-jest@29.1.1_@babel+core@7.22.10_jest@29.6.3_typescript@5.1.6/node_modules:/Users/matthewcarr_1/Documents/Github/snaptime/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/matthewcarr_1/Documents/Github/snaptime/node_modules/.pnpm/ts-jest@29.1.1_@babel+core@7.22.10_jest@29.6.3_typescript@5.1.6/node_modules/ts-jest/node_modules:/Users/matthewcarr_1/Documents/Github/snaptime/node_modules/.pnpm/ts-jest@29.1.1_@babel+core@7.22.10_jest@29.6.3_typescript@5.1.6/node_modules:/Users/matthewcarr_1/Documents/Github/snaptime/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../ts-jest/cli.js" "$@"
else
  exec node  "$basedir/../ts-jest/cli.js" "$@"
fi
